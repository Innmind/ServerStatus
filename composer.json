{
    "name": "innmind/server-status",
    "type": "library",
    "description": "Library to access various components of the operating system",
    "homepage": "http://github.com/Innmind/ServerStatus",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/ServerStatus/issues"
    },
    "require": {
        "php": "~8.1",
        "innmind/immutable": "~4.0",
        "innmind/time-continuum": "~3.0",
        "innmind/url": "~4.0",
        "psr/log": "~3.0",
        "innmind/server-control": "dev-capabilities"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Innmind\\Server\\Status\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Server\\Status\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "vimeo/psalm": "~4.30",
        "innmind/coding-standard": "~2.0"
    }
}
