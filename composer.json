{
    "name": "innmind/server-status",
    "type": "library",
    "description": "Library to access various components of the operating system",
    "homepage": "http://github.com/Innmind/ServerStatus",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/ServerStatus/issues"
    },
    "require": {
        "php": "~8.4",
        "innmind/immutable": "~6.0",
        "innmind/time-continuum": "~5.0",
        "innmind/time-warp": "~5.0",
        "innmind/url": "~5.0",
        "psr/log": "~3.0",
        "innmind/server-control": "~7.0",
        "innmind/validation": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Server\\Status\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Server\\Status\\": "tests/"
        }
    },
    "require-dev": {
        "innmind/black-box": "~6.1",
